{% spaceless %}
    {% if title is defined and title|length > 0 %}
        <title>{{ title }}</title>
    {% endif %}

    {% for meta, value in metas %}
        {% if value|length > 0 %}
            <meta name="{{ meta }}" content="{{ value }}"/>
        {% endif %}
    {% endfor %}

    {% if ogs is defined %}
        {% for og, value in ogs %}
            {% if value|length > 0 %}
                <meta property="og:{{ og }}" content="{{ value }}" />
            {% endif %}
        {% endfor %}
    {% endif %}
{% endspaceless %}


